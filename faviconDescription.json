

{
	"masterPicture": "./public/logo_lighty.png",
	"iconsPath": "/",
	"design": {
		"ios": {
			"pictureAspect": "backgroundAndMargin",
			"backgroundColor": "#1017cb",
			"margin": "21%",
			"assets": {
				"ios6AndPriorIcons": false,
				"ios7AndLaterIcons": false,
				"precomposedIcons": false,
				"declareOnlyDefaultIcon": true
			}
		},
		"desktopBrowser": {
			"design": "background",
			"backgroundColor": "#1017cb",
			"backgroundRadius": 0.45,
			"imageScale": 0.8
		},
		"windows": {
			"pictureAspect": "noChange",
			"backgroundColor": "#1017cb",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": false,
				"windows10Ie11EdgeTiles": {
					"small": false,
					"medium": true,
					"big": false,
					"rectangle": false
				}
			}
		},
		"androidChrome": {
			"pictureAspect": "noChange",
			"themeColor": "#1017cb",
			"manifest": {
				"display": "standalone",
				"orientation": "notSet",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": false,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "blackAndWhite",
			"threshold": 90.9375,
			"themeColor": "#1017cb"
		}
	},
	"settings": {
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false,
		"readmeFile": false,
		"htmlCodeFile": false,
		"usePathAsIs": false
	}
}

